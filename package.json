    "babel-plugin-module-resolver": "^4.1.0",
    "contentful": "^8.3.5",
    "dayjs": "^1.10.5",
    "next": "11.0.1",
    "next-seo": "^4.24.0",
    "next-themes": "^0.0.14",
    "nightwind": "^1.1.12",
    "postcss": "^8.2.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.32.12",
    "tailwindcss": "^2.1.2"
