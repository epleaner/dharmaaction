  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev -p 8081",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
